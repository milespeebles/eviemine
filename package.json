{
  "name": "eviemine",
  "version": "0.1.0",
  "license": "MIT",
  "engines": {
    "node": "10"
  },
  "dependencies": {
    "@emotion/core": "10.0.22",
    "@rebass/forms": "4.0.6",
    "@reduxjs/toolkit": "1.0.4",
    "@zeit/next-css": "1.0.1",
    "crocks": "0.12.4",
    "emotion-theming": "10.0.19",
    "firebase": "7.5.0",
    "firebase-admin": "8.8.0",
    "firebase-functions": "3.3.0",
    "next": "9.1.4",
    "next-offline": "4.0.6",
    "nookies": "2.0.8",
    "object-key-mirror": "1.0.5",
    "ramda": "0.26.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-hook-form": "3.28.9",
    "react-redux": "7.1.3",
    "react-redux-firebase": "3.0.5",
    "rebass": "4.0.7",
    "redux": "4.0.4",
    "redux-devtools-extension": "2.13.8",
    "redux-firestore": "0.11.0",
    "redux-logger": "3.0.6",
    "redux-persist": "6.0.0",
    "rxjs": "6.5.3",
    "rxjs-hooks": "0.5.2",
    "sanitize.css": "11.0.0",
    "toolbelt": "git://github.com/milespeebles/toolbelt.git"
  },
  "devDependencies": {
    "@babel/cli": "7.7.4",
    "@babel/core": "7.7.4",
    "@babel/preset-env": "7.7.4",
    "@storybook/react": "5.2.7",
    "babel-loader": "8.0.6",
    "concurrently": "5.0.0",
    "cpx": "1.5.0",
    "firebase-tools": "7.8.1",
    "next-transpile-modules": "2.3.1",
    "rimraf": "3.0.0",
    "webpack": "4.41.2"
  },
  "scripts": {
    "storybook": "start-storybook --ci --port 8000",
    "dev": "concurrently npm:local npm:serve npm:storybook",
    "local": "next 'src/app'",
    "serve": "NODE_ENV=production firebase serve --host 0.0.0.0 --only functions,hosting",
    "preserve": "yarn build-public && yarn build-functions && yarn build-app && yarn copy-deps && yarn install-deps",
    "deploy": "NODE_ENV=production firebase deploy",
    "clean": "rimraf 'dist/functions' && rimraf 'dist/public'",
    "build-public": "cpx 'src/public/**/*.*' 'dist/public' -C",
    "build-functions": "babel 'src/functions' --out-dir 'dist/functions'",
    "build-app": "NODE_ENV=production next build 'src/app'",
    "copy-deps": "cpx '*{package.json,yarn.lock}' 'dist/functions'",
    "install-deps": "cd 'dist/functions' && yarn",
    "firebase-login": "firebase login"
  }
}
